{
  "name": "interactive_map",
  "version": "2.0.0",
  "description": "Interactive map for search departments position and staff",
  "source": "src/client/index.tsx",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve --config webpack.config.ts",
    "build": "NODE_ENV=production webpack --config webpack.config.ts",
    "start": "docker compose up -d && open http://localhost/",
    "server-local": "yarn tsc && pm2-runtime start server",
    "server-restart": "yarn tsc && docker exec -it interactivemap /var/www/node_modules/.bin/pm2-runtime restart server",
    "server-logs": "docker exec -it interactivemap /var/www/node_modules/.bin/pm2 log server",
    "tsc": "tsc --project ./tsconfig.server.json",
    "serviceWorker": "tsc --project ./tsconfig.serviceWorker.json",
    "postbuild": "workbox generateSW",
    "svg": "svgr -d ./src --ext tsx ./src/",
    "test": "jest --watchAll",
    "coverage": "test -- --coverage",
    "precommit": "lint-staged"
  },
  "keywords": [
    "react",
    "testing"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "author": "@abelikov",
  "license": "ISC",
